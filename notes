primer = None
        tmpW = []
        tmpb = []
        for index in range(len(A)-1,-1,-1):
            currA = A[index]
            if(index == len(A)-1):
                primer = self.dOutput(currA)
            else:
                primer = self.dhidden(self.W[index+1], currA, primer)
            #dont update as used in later execution
            tmp = np.dot(np.dot(primer,self.dLinCombW(currA)), (1/self.n))
            print(tmp)
            tmpW.append(tmp)
            tmp = np.dot(np.dot(primer,self.dLinCombB(currA)), (1/self.n))
            print(tmp)
            tmpb.append(tmp)
        tmpW.reverse()
        self.W = tmpW
        tmpb.reverse()
        self.b = tmpb